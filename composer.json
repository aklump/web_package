{
  "name": "aklump/web-package",
  "description": "A shell script to help the management of files as a package.",
  "version": "3.2.5",
  "authors": [
    {
      "name": "Aaron Klump",
      "email": "sourcecode@intheloftstudios.com"
    }
  ],
  "homepage": "http://www.intheloftstudios.com/packages/bash/web_package",
  "require": {
    "php": "^7.3",
    "symfony/yaml": "^5.4",
    "aklump/loft-lib": "^2",
    "twig/twig": "^2.6",
    "z4kn4fein/php-semver": "^2.0",
    "ext-json": "*",
    "symfony/config": "^5.4",
    "symfony/filesystem": "^5.4",
    "symfony/console": "^5.4",
    "symfony/serializer": "^5.4",
    "symfony/event-dispatcher": "^5.4",
    "webmozart/glob": "^4.6",
    "jawira/case-converter": "^3.5",
    "aklump/local-timezone": "^0.0.1",
    "symfony/validator": "^5.4",
    "league/container": "^4.2"
  },
  "autoload": {
    "psr-4": {
      "AKlump\\WebPackage\\": "src",
      "AKlump\\LoftLib\\Component\\Config\\": "lib/loft_php_lib/dist/src/AKlump/LoftLib/Component/Config/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AKlump\\WebPackage\\Tests\\": "tests_unit"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
